    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "vlucas/phpdotenv": "^5.3",
        "ext-pdo": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": ">=8.5",
        "phpstan/phpstan": "^1.10"
